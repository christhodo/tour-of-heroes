{
  "projectType": "library",
  "sourceRoot": "libs/environment/src",
  "prefix": "tour-of-heroes-app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/environment"],
      "options": {
        "jestConfig": "libs/environment/jest.config.ts",
        "passWithNoTests": true
